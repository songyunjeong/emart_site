\n\n/*-------- category_box --------*/\n.food_fruit .category_box {\n  width: 1240px; height: 170px;\n  border: 1px solid #ccc;\n  box-sizing: border-box;\n  margin-bottom: 50px;\n  overflow: hidden;\n}\n\n.category_box .category {\n  font-size: 14px;\n  font-weight: bold;\n  padding: 15px 0 15px 20px;\n  box-sizing: border-box;\n}\n.category_box .category_li ul {\n  width: 285px; height: 100px;\n  font-size: 14px;\n  float: left;\n  margin-left: 20px;\n}\n.category_box .category_li li {margin-bottom: 5px;}\n.category_box .category_li li:last-child {margin-bottom: 0;}\n.category_box .category_li li a:hover {\n  font-weight: 600;\n}\n\n\n/*-------- product_box --------*/\n.product_box {\n  display: flex;\n  flex-wrap: wrap;\n  width: 1240px;\n}\n.product_box .product {\n  margin-right: 20px;\n}\n.product_box .product:nth-child(4n) {\n  margin-right: 0;\n  margin-bottom: 20px;\n}\n\n.product_box .img {\n  width: 295px; height: 350px;\n  margin-bottom: 10px;\n}\n.product_box .text {\n  width: 295px; height: 100px;\n  font-size: 0;\n}\n.product_box .text li {font-size: 12px;}\n.product_box .text .emart_mall {\n  display: inline-block;\n  width: 55px; height: 20px;\n  background-color: #FFD140;\n  font-weight: bold;\n  text-align: center;\n  line-height: 20px;\n  margin-right: 5px;\n  margin-bottom: 5px;\n}\n.product_box .text .delivery {\n  display: inline-block;\n  width: 55px; height: 20px;\n  background-color: #8496AD;\n  color: white;\n  text-align: center;\n  line-height: 20px;\n  margin-right: 5px;\n  margin-bottom: 5px;\n}\n.product_box .text .fresh {\n  display: inline-block;\n  color: red;\n}\n.product_box .text .emart_fresh {\n  font-weight: bold;\n}\n.product_box .text .price {\n  display: inline-block;\n  font-size: 20px;\n  font-weight: bold;\n  margin-right: 2px;\n}\n.product_box .text .won {\n  display: inline-block;\n}\n.product_box .text .sale {\n  display: inline-block;\n  color: #666;\n  text-decoration: line-through;\n  margin-left: 10px;\n}\n.product_box .text .sale_won {\n  display: inline-block;\n  color: #666;\n  margin-left: 2px;\n}\n\n\n/*-------- page_nation --------*/\n#page_left, #page01, #page02, #page03, #page04, #page05, #page06, #page07, #page08, #page09, #page10, #page_next, #page_right {display: none;}\n\n#page01:checked ~ .label label:nth-child(2),\n#page02:checked ~ .label label:nth-child(3),\n#page03:checked ~ .label label:nth-child(4),\n#page04:checked ~ .label label:nth-child(5),\n#page05:checked ~ .label label:nth-child(6),\n#page06:checked ~ .label label:nth-child(7),\n#page07:checked ~ .label label:nth-child(8),\n#page08:checked ~ .label label:nth-child(9),\n#page09:checked ~ .label label:nth-child(10),\n#page10:checked ~ .label label:nth-child(11) {\n  background-color: black;\n  color: white;\n}\n\n.page_nation {\n  width: 1240px; height: 30px;\n  margin: 100px auto;\n}\n.page_nation .label {text-align: center;}\n.page_nation .label label {\n  display: inline-block;\n  width: 30px; height: 30px;\n  border: 1px solid #ccc;\n  box-sizing: border-box;\n  font-size: 12px;\n  text-align: center;\n  line-height: 28px;\n}\n.page_nation .label label:nth-child(1),\n.page_nation .label label:nth-child(12),\n.page_nation .label label:nth-child(13) {background-color: #eee;}\n.page_nation .label label:hover {cursor: pointer;}"},"lineCount":null}},"error":null,"hash":"006dc43df181d375a824ee9e59edbb0b","cacheData":{"env":{}}}