n  font-size: 12px;\r\n  color: firebrick;\r\n  position: absolute;\r\n  top: 35px; left: 157px;\r\n}\r\n\r\n.info .pw_re {\r\n  position: relative;\r\n}\r\n.info .pw_re_message {\r\n  width: 300px;\r\n  font-size: 12px;\r\n  color: firebrick;\r\n  position: absolute;\r\n  top: 35px; left: 157px;\r\n}\r\n\r\n\r\n/*-------- info member --------*/\r\n.member_info .phone select {\r\n  margin-right: 20px;\r\n}\r\n.member_info .phone input {\r\n  width: 66px;\r\n  text-align: center;\r\n}\r\n.member_info .phone .tel {\r\n  width: 5px;\r\n  margin: 0 15px;\r\n}\r\n\r\n\r\n/*-------- info like --------*/\r\n.like_info .sub_title span {\r\n  color: #999;\r\n  font-size: 12px;\r\n  font-weight: 500;\r\n  margin-left: 10px;\r\n}\r\n.like_info fieldset {\r\n  font-size: 12px;\r\n  margin-bottom: 20px;\r\n  padding: 10px;\r\n  box-sizing: border-box;\r\n}\r\n.like_info fieldset legend {\r\n  font-size: 14px;\r\n  padding: 0 10px;\r\n  box-sizing: border-box;\r\n}\r\n.like_info fieldset input {\r\n  margin-right: 5px;\r\n}\r\n.like_info fieldset p {\r\n  display: inline-block;\r\n  width: 105px;\r\n}\r\n\r\n\r\n/*-------- stipulation --------*/\r\n.join .stipulation .sub_title {\r\n  margin-bottom: 20px;\r\n}\r\n.join .stipulation > div {\r\n  margin-bottom: 10px;\r\n}\r\n.join .stipulation > div:nth-child(2) {\r\n  font-weight: bold;\r\n}\r\n.join .stipulation > div input {\r\n  display: inline-block;\r\n  margin-right: 10px;\r\n}\r\n.join .stipulation > div input:hover {\r\n  cursor: pointer;\r\n}\r\n.join .stipulation > div p {\r\n  display: inline-block;\r\n}\r\n\r\n\r\n/*-------- join_btn --------*/\r\n.join .join_btn {\r\n  display: block;\r\n  width: 300px; height: 50px;\r\n  background-color: #eee;\r\n  outline: none;\r\n  font-size: 20px;\r\n  border: 1px solid #ccc;\r\n  cursor: pointer;\r\n  margin: 100px auto 150px;\r\n}"},"lineCount":173}},"error":null,"hash":"0b35357a6d338e08e0274b7942f23404","cacheData":{"env":{}}}